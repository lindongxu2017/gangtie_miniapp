<view class="songhui_bg">
	<image class="logo" src="/image/songhui_logo.png"></image>
</view>
<view class="songhui_title">
	<image src="/image/songhui_title.png"></image>
</view>
<view class="songhui_index_box">
	<view class="songhui_index_box_top"></view>
	<view class="songhui_index_box_bottom">
		<view class="songhui_index_box_bottom_left"></view>
		<view class="songhui_index_box_bottom_right"></view>
	</view>
</view>
<view class="songhui_index songhui_list">
	<navigator class="songhui_item" wx:for="{{list}}" wx:key="{{index}}" url="{{item.url}}" hover-class='none' bindtap="{{item.url ? '' : 'hint'}}">
		<view><image src="{{item.icon}}"></image></view>
		<view>{{item.title}}</view>
	</navigator>
</view>