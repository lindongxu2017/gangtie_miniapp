.list{
	padding:0 30rpx;
}
.item{
	padding:30rpx 0;
	border-bottom:1rpx solid #e5e5e5;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.item .name{
	line-height: 28rpx;
	font-size: 28rpx;
	color:#333;
}
.item image{
	width: 40rpx;
	height: 40rpx;
}

image.bg{
	width:100%;
	display:block;
}